   "source": [
    "S = input()\n",
    "S=''.join(sorted(S))     # 문자열 리스트로 정렬 후 문자열(띄어쓰기 없이)로 만들기\n",
    "result = []\n",
    "\n",
    "num = 0\n",
    "\n",
    "for s in S:\n",
    "    #\n",
    "    if s.isdigit():\n",
    "        num += int(s)\n",
    "    else:\n",
    "        result.append(s)\n",
    "if num != 0:\n",
    "    result.append(str(num))\n",
    "\n",
    "print(''.join(result))"
   ]
  }
